{
  "name": "voc-email-template",
  "version": "1.0.0",
  "description": "The Weekly VoC Email template that is currently sent via SendGrid",
  "main": "render-handlebar.js",
  "scripts": {
    "render": "node render-handlebar.js --data data.json --templateFile ./output/email-cdn.html",
    "watch": "watch 'npm run render' ./ --ignoreDirectoryPattern=/output/"
